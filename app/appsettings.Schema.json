{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Schema for appsettings.json file",
    "required": [
        "token",
        "url"
    ],
    "properties": {
        "token": {
            "$id": "#/properties/token",
            "type": "string",
            "title": "The Token Schema",
            "description": "Bot token from Telegram",
            "default": "",
            "examples": [
                "12345678900:BBGh6k_gfhkdjghsfjkd534"
            ]
        },
        "url": {
            "$id": "#/properties/url",
            "type": "string",
            "title": "The Url Schema",
            "description": "Base url for your api",
            "default": "http://localhost:5000",
            "examples": [
                "http://localhost:5000"
            ]
        }
    }
}